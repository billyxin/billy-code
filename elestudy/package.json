{
	"name": "elestudy",
	"version": "1.0.0",
	"description": "des",
	"main": "./win/main.js",
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"electron": "^11.1.1",
		"electron-builder": "^22.9.1"
	},
	"dependencies": {
		"jquery": "^3.5.1"
	},
	"build": {
		"productName": "elestudy2233",
		"appId": "com.elestudy.test",
		"copyright": "copyrights reserved",
		"asar": true,
		"win": {
			"target": [
				"portable"
			],
			"icon": "billy.ico"
		},
		"extraFiles": [
			{
				"from": "./dict.txt2",
				"to": "./dict.txt"
			}
		],
		"extraResources": [
			{
				"from": "./images",
				"to": "./images"
			},
			{
				"from": "./",
				"to": "./",
				"filter": [
					"dict.txt"
				]
			}
		],
		"files": [
			"!**/*.jpg",
			"!./doc/*"
		]
	},
	"scripts": {
		"start": "set NODE_ENV=product&&electron ./win/main.js",
		"dev": "set NODE_ENV=dev&&electron ./win/main.js",
		"build": "set NODE_ENV=product&&electron-builder --win --x64 --dir"
	}
}
